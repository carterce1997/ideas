
library(tidyverse)
library(sf)
library(carter)

emc <- 
  tibble(geometry = 'POLYGON((-73.0365 44.0627,-73.0364 44.0622,-73.0360 44.0600,-73.0381 44.0598,-73.0383 44.0608,-73.0399 44.0607,-73.0397 44.0596,-73.0418 44.0594,-73.0419 44.0599,-73.0419 44.0605,-73.0419 44.0608,-73.0420 44.0617,-73.0418 44.0625,-73.0417 44.0630,-73.0417 44.0631,-73.0416 44.0637,-73.0414 44.0641,-73.0406 44.0639,-73.0405 44.0638,-73.0403 44.0638,-73.0402 44.0637,-73.0400 44.0637,-73.0398 44.0636,-73.0395 44.0634,-73.0393 44.0634,-73.0391 44.0634,-73.0388 44.0634,-73.0383 44.0633,-73.0379 44.0632,-73.0376 44.0631,-73.0373 44.0630,-73.0370 44.0630,-73.0368 44.0628,-73.0365 44.0627))') %>%
  st_as_sf(wkt = 'geometry')

footprint <-
  tibble(geometry = 'MULTIPOLYGON (((-72.81003 43.24217192284553, -72.77014 43.34564669033547, -72.71231 43.49140822944084, -72.65494 43.63724005672841, -72.597595 43.78301799247088, -72.5397 43.92858955110797, -72.482574 44.07432860701254, -72.4538 44.14646748467885, -72.4249 44.21907674553511, -72.42291 44.224051299426634, -73.74753 44.24655093494868, -73.767975 43.25815531909012, -72.81003 43.24217192284553)))') %>%
  st_as_sf(wkt = 'geometry')

ggplot() +
  geom_sf(data = footprint) +
  geom_sf(data = emc, fill = 'red') +
  theme_carter()
